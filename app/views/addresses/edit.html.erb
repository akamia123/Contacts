<% form_for :address, :url => contact_address_url(@contact.id, @address.id), :html => {:method => :put} do |f| %>
    <table> 
        <%= render :partial => "form", :locals => {:f => f} %>
        <tr>
            <td></td>
            <td><%= submit_tag "Update Address" %></td>
        </tr>
    </table>
<% end %>
